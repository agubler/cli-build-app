{
	"build-app": {
		"compression": [ "gzip", "brotli" ],
		"locale": "en",
		"supportedLocales": [ "es" ],
		"features": {
			"env": "ci",
			"foo": true,
			"bar": false
		},
		"bundles": {
			"ignored": [
				"src/Bar"
			],
			"lazy": [
				"**/LazyWidget"
			],
			"widgets": [
				"src/Bar",
				"src/Baz"
			]
		},
		"build-time-render": {
			"root": "app"
		},
		"externals": {
			"dependencies": [ "foo" ]
		}
	}
}
